    "docs": "mr-doc -s modules/ -o docs/ -n 'MERNjs'",
    "doc": "webpack-dev-server"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "jade": "^1.11.0",
    "jquery": "^2.2.4",
    "jsonwebtoken": "^7.1.7",
    "lodash": "^4.14.1",
    "moment": "^2.14.1",
    "mongoose": "^4.5.8",
    "morgan": "^1.7.0",
    "react": "^15.3.0",
    "react-cookie": "^0.4.7",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "redux": "^3.5.2",
    "sanitize-html": "^1.13.0",
    "serve-favicon": "^2.3.0",
    "toastr": "^2.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
