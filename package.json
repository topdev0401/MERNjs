{
  "name": "MERNjs",
  "version": "0.0.0",
  "description": "A Scaffolding of MERN (MongoDB, ExpressJS, ReactJS and NodeJS) stack.",
  "keywords": [
    "MongoDB",
    "ExpressJS",
    "ReactJS",
    "NodeJS",
    "Redux",
    "Webpack",
    "ECMAScript 2015 (ES6)"
  ],
  "author": "Muhammad Mohaiminul Islam <IamMohaiminul@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/IamMohaiminul/MERNjs"
  },
  "engines": {
    "node": "4.4.7"
  },
  "main": "./bin/www",
  "scripts": {
    "start": "babel-node ./bin/www",
    "postinstall": "webpack",
    "nodemon": "nodemon --exec babel-node ./bin/www",
    "watch": "webpack --watch",
    "debug": "DEBUG=MERNjs:* npm run nodemon",
    "debug-win": "SET DEBUG=MERNjs:* & npm run nodemon",
    "docs": "mr-doc -s modules/ -o docs/ -n 'MERNjs'",
    "doc": "webpack-dev-server"
  },
  "dependencies": {
    "bcrypt-nodejs": "~0.0.3",
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.14.0",
    "jquery": "~2.2.4",
    "jsonwebtoken": "~7.1.7",
    "lodash": "~4.14.1",
    "moment": "~2.14.1",
    "mongoose": "~4.5.8",
    "morgan": "~1.7.0",
    "react": "~15.3.0",
    "react-cookie": "~0.4.7",
    "react-dom": "~15.3.0",
    "react-redux": "~4.4.5",
    "react-router": "~2.6.1",
    "redux": "~3.5.2",
    "sanitize-html": "~1.13.0",
    "serve-favicon": "~2.3.0",
    "toastr": "~2.1.2"
  },
  "devDependencies": {
    "babel-cli": "~6.11.4",
    "babel-core": "~6.11.4",
    "babel-loader": "~6.2.4",
    "babel-preset-es2015": "~6.9.0",
    "babel-preset-react": "~6.11.1",
    "css-loader": "~0.23.1",
    "mr-doc": "~3.2.1",
    "node-sass": "~3.8.0",
    "nodemon": "~1.10.0",
    "sass-loader": "~4.0.0",
    "style-loader": "~0.13.1",
    "webpack": "~1.13.1",
    "webpack-dev-server": "~1.14.1"
  }
}
