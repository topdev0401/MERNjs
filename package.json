{
  "name": "MERNjs",
  "version": "0.0.0",
  "description": "MongoDB, ExpressJS, ReactJS and NodeJS with ECMAScript 2015 (ES6)",
  "keywords": [
    "MongoDB",
    "ExpressJS",
    "ReactJS",
    "NodeJS",
    "ECMAScript 2015 (ES6)"
  ],
  "author": "Muhammad Mohaiminul Islam <IamMohaiminul@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
