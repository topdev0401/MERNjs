{
  "name": "MERNjs",
  "version": "0.0.0",
  "description": "A Scaffolding of MongoDB, ExpressJS, ReactJS and NodeJS with Redux and ECMAScript 2015 (ES6)",
  "keywords": [
    "MongoDB",
    "ExpressJS",
    "ReactJS",
    "NodeJS",
    "Redux",
    "Webpack",
    "ECMAScript 2015 (ES6)"
  ],
  "author": "Muhammad Mohaiminul Islam <IamMohaiminul@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/IamMohaiminul/MERNjs"
  },
  "main": "./bin/www",
  "scripts": {
    "start": "nodemon --exec babel-node ./bin/www",
    "debug": "DEBUG=MERNjs:* npm run start",
    "debug-win": "SET DEBUG=MERNjs:* & npm run start",
